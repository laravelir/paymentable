{
    "name": "laravelir/paymentable",
    "description": "payment service",
    "homepage": "https://github.com/laravelir/paymentable",
    "type": "library",
    "version": "0.1.0",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintainer",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Paymentable\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelir\\Paymentable\\Providers\\PaymentableServiceProvider"
            ],
            "aliases": {
                "Paymentable": "Laravelir\\Paymentable\\Facades\\Paymentable"
            }
        }
    },
    "require": {
